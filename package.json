        "closure-webpack-plugin": "^2.0.1",
        "copy-webpack-plugin": "^5.1.2",
        "css-loader": "^3.6.0",
        "elm": "^0.19.1-3",
        "elm-analyse": "^0.16.5",
        "elm-format": "^0.8.4",
