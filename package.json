    "description": "Pokemon PVP team manager",
    "main": "index.js",
    "scripts": {
        "test": "elm-test",
        "start": "npm run dev",
        "dev": "webpack-dev-server --hot --colors --port 3000",
        "build": "webpack",
        "prod": "export NODE_ENV=production && webpack -p",
